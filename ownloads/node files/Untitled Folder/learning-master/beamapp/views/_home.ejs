<!DOCTYPE html>
<html>
    <head>
        <!-- <link rel="stylesheet" media="screen" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" /> -->
        <!-- <link rel="stylesheet" media="screen" href="https://unpkg.com/todomvc-app-css@2.0.6/index.css" /> -->
        <link rel="stylesheet" media="screen" href="./css/app.css" />
        <script src="./js/vue2.5.17.js"></script>
        <title>BeamApp</title>
    </head>
    <body>
        <section class="beamApp">
            <header class="header">
                <h1>BeamApp</h1>
                <input class="new-todo" autofocus autocomplete="off" placeholder="What needs to be done?" v-model="newTodo"
                    @keyup.enter="addTodo">
            </header>
            <section class="main" v-show="todos.length" v-cloak>
            </section>
            <div id="googleMap" style="width:100%;height:400px;"></div>
        </section>
        
        <script>
            function myMap() {
                var mapProp= {
                    center:new google.maps.LatLng(1.351376,103.805181),
                    zoom:13,
                };
                var map=new google.maps.Map(document.getElementById("googleMap"),mapProp);
            }
        </script>

        <script src="https://maps.googleapis.com/maps/api/js?key=<%= map_key %>&callback=myMap"></script>
    </body>
</html>